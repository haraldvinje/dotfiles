{
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.enableNeovim": true,
  "vim.vimrc.enable": true,
  "editor.mouseWheelZoom": true,
  "keyboard.dispatch": "keyCode",
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-k>": false,
    "<C-shift+`>": false,
    "<C-b>": false,
    "<C-f>": false,
    "<C-n>": false,
    "<C-c>": false,
    "<C-p>": false,
  },
  "editor.lineNumbers": "relative",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/bin/bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "/bin/zsh"
    },
    "fish": {
      "path": "/bin/fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "terminal.external.linuxExec": "truecolor",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "material-product-icons",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "json.schemas": [],
  "gitlens.hovers.currentLine.over": "line",
  "cloudcode.autoDependencies": "off",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.linkedEditing": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "material-icon-theme.activeIconPack": "react",
  "workbench.colorTheme": "Gruvbox DrakenLords Dark",
  "yaml.schemas": {
    "file:///home/haraldv/.vscode/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "redhat.telemetry.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.fontFamily": "'Hack Nerd Font'",
  "editor.inlineSuggest.enabled": true,
  "svelte.enable-ts-plugin": true,
  "aws.samcli.enableCodeLenses": false,
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "bottom",
  "material-icon-theme.folders.theme": "specific",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "reactSnippets.settings.prettierEnabled": true,
  "reactSnippets.settings.importReactOnTop": false,
  "remote.SSH.defaultExtensions": [
    "gitpod.gitpod-remote-ssh"
  ],
  "search.exclude": {
    "**/build": true
  },
  "aws.resources.enabledResources": [
    "AWS::EC2::Host",
    "AWS::EC2::KeyPair",
    "AWS::EC2::Subnet",
    "AWS::EC2::VPC"
  ],
  "azureResourceGroups.groupBy": "resourceType",
  "vim.normalModeKeyBindingsNonRecursive": [
    // Undo & Redo
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    },
    {
      "before": [
        "c-r"
      ],
      "commands": [
        "redo"
      ]
    }
  ],
  "githubPullRequests.pullBranch": "never",
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.minimap.enabled": false,
  "git.defaultBranchName": "master",
  "workbench.editor.tabSizingFixedMaxWidth": 160,
  "workbench.editor.tabSizing": "fixed",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "aws.suppressPrompts": {
    "codeWhispererNewWelcomeMessage": true
  }
}